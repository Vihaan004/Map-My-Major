{
  "version": 2,
  "name": "map-my-major",
  "builds": [
    {
      "src": "app/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "functions": {
    "app/backend/src/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app/backend/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app/frontend/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
