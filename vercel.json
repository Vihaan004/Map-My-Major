{
  "version": 2,
  "name": "map-my-major",
  "buildCommand": "cd app/frontend && npm run build",
  "outputDirectory": "app/frontend/dist",
  "functions": {
    "app/backend/api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app/backend/api/$1.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
